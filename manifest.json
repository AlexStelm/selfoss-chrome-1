{
	"manifest_version": 2,
	
	"name": "__MSG_selfosschrome_name__",
	"version": "1.0",

	"default_locale": "en",
	"description": "__MSG_selfosschrome_description__",
	
	"icons": {
		"128": "selfoss_128.png"
	},
  
	"background": {
		"persistent": false,
		"page": "background.html"
	},

	"browser_action": {
		"default_icon": "selfoss_disabled.png"
	},

	"options_page": "options.html",
  
	"permissions": [
		"alarms",
		"tabs",
		"webNavigation",
		"http://*/*",
		"https://*/*"
	]
}
